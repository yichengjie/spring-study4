<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	">

    <bean id="simpleBean"
          class="com.yicj.study.foo.model.MySimpleBean">
        <property name="connectionString" value="bollocks" />
        <property name="username" value="Microsoft" />
        <property name="password" value="imaginecup" />
     </bean>

    <bean id="bfpp"
          class="com.yicj.study.beanfactorypostprocessor.ObscenityRemovingBeanFactoryProcessor">
        <property name="obscenities">
            <set>
                <value>bollocks</value>
                <value>winky</value>
                <value>bum</value>
                <value>Microsoft</value>
            </set>
        </property>
    </bean>

</beans>